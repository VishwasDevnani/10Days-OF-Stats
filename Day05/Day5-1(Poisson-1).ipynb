    "# Enter your code here. Read input from STDIN. Print output to STDOUT\n",
    "\n",
    "def fact(n):\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    else: \n",
    "        return n*fact(n-1)\n",
    "\n",
    "        \n",
    "\n",
    "e=2.71828182846\n",
    "print(\"{0:.3f}\".format((e**(-2.5))*(2.5**5)/fact(5)))"
   ]
