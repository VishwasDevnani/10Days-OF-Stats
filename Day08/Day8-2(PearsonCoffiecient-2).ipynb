    "by = -3/4\n",
    "r² = bx * by\n",
    "r² = -3/4 * -3/4\n",
    "r² = 9/16\n",
    "r +/- 3/4\n",
    "if bx = by then\n",
    "  r = -3/4\n",
    "\"\"\"\n",
    "\n",
    "print (\"Result is: {}\".format(\"-3/4\"))"
